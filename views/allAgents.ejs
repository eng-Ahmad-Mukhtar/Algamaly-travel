<!DOCTYPE html>
<html lang="en" dir="rtl">

<head>
    <meta charset="utf-8">
    <title>صفحة الادارة</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">

    <!-- Favicon -->
    <link href="/img/favicon.ico" rel="icon">
    <link rel="icon" href="/img/snapedit_1723081495665.png" type="image/icon type">

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Heebo:wght@400;500;600&family=Inter:wght@700;800&display=swap" rel="stylesheet">
    
    <!-- Icon Font Stylesheet -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
    <link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/themes/smoothness/jquery-ui.css">
    <link href="/lib/animate/animate.min.css" rel="stylesheet">
    <link href="/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">

    <!-- Customized Bootstrap Stylesheet -->
    <link href="/css/bootstrap.min.css" rel="stylesheet">

    <!-- Template Stylesheet -->
    <link href="/css/style.css" rel="stylesheet">

</head>

<body>
    <div class="container-xxl bg-white p-0">
        <!-- Spinner Start -->
        <div id="spinner" class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center">
            <div class="spinner-border text-primary" style="width: 3rem; height: 3rem;" role="status">
                <span class="sr-only">Loading...</span>
            </div>
        </div>
        <!-- Spinner End -->


        <!-- Navbar Start -->
        <div class="container-fluid nav-bar bg-transparent">
            <nav class="navbar navbar-expand-lg bg-white navbar-light py-0 px-4">
                <a href="/" class="navbar-brand d-flex align-items-center text-center">
                    <div class="icon p-2 me-2">
                        <img class="img-fluid" src="img/snapedit_1723081495665.png" alt="Icon" style="width: 85px;
    height: 59px;">
                    </div>
                    <h1 class="m-0 text-primary">الجملى تراڤيل </h1>
                </a>
                <button type="button" class="navbar-toggler" data-bs-toggle="collapse" data-bs-target="#navbarCollapse">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarCollapse">
                    <div class="navbar-nav ms-auto">
                        <a href="/" class="nav-item nav-link">الصفحة الرئيسية</a>
                        <a href="/login" class="nav-item nav-link active">اضافة جديدة</a>
                        <div class="nav-item dropdown">
                            <a href="#" class="nav-link dropdown-toggle" data-bs-toggle="dropdown">بحث</a>
                            <div class="dropdown-menu rounded-0 m-0">
                                <a href="/subiscription_information" class="dropdown-item">بالأسم </a>
                                <a href="/subiscription_information_by_id" class="dropdown-item">برقم الجواز </a>
                                <a href="/subiscription_information_by_direction" class="dropdown-item"> بالوجهة</a>
                                <a href="/allAgents" class="dropdown-item"> جميع العملاء</a>
                            </div>
                        </div>
                     
                        <a href="/destroy" class="nav-item nav-link">تسجيل خروج</a>
                        <a href="#" class="nav-item nav-link"> <%= loged_user.name  %> مرحبا</a>
                    </div>
              
                </div>
            </nav>
        </div>
        <!-- Navbar End -->




       


        <!-- Contact Start -->
        <div class="container-xxl py-5">
            <div class="container">
                <div class="text-center mx-auto mb-5 wow fadeInUp" data-wow-delay="0.1s" style="max-width: 600px;">
                    <h1 class="mb-3">جميع العملاء</h1>
                </div>
            </div>
        </div>

     
        <!-- Contact End -->













        
     
        <% data.forEach(student=> { %>
            <label for="">   تفاصيل الرحلة  :  </label>
            <%= student.travel_details %>
            <% student.subscriptions.forEach((one, index)=> { %>
        <table class="fl-table">
            <thead>
            <tr>
                <th> الأسم  </th>
                <th>رقم الجواز </th>
                <th> اسم الوكيل </th>
                <th>الوجهة</th>
                <th> الحالة</th>
                <th>رقم الهاتف </th>
                <th>تاريخ الاضافه </th>
                <th>ملاحظات </th>
            </tr>
            </thead>
            <tbody>
            <tr>
                <td><%=one.name%></</td>
                <td><%=one.passportId%></</td>
                <td><%=one.directorName%></td>
                <td><%= one.direction %></td>
                <td><%=one.status %></td>
                <td><%=one.phoneNumber%></td>
                <td><%=one.timestamps%></td>
                <td><%=one.notes%></td>
               

            </tr>
        
       
            <tbody>
        </table>
        <% }) %>
<br>
<hr>
<br>
        <% }) %>
        

        








        <!-- Back to Top -->
        <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i class="bi bi-arrow-up"></i></a>
    </div>

    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="/lib/wow/wow.min.js"></script>
    <script src="/lib/easing/easing.min.js"></script>
    <script src="/lib/waypoints/waypoints.min.js"></script>
    <script src="/lib/owlcarousel/owl.carousel.min.js"></script>

    <!-- Template Javascript -->
    <script src="js/main.js"></script>
    	<!-- jquery -->
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <!-- Include jQuery UI -->
    <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js"></script>
	<script>

	

$(function() {
            $('#direction').autocomplete({
                source: function(request, response) {
                    $.ajax({
                        url: '/autocompleteee',
                        dataType: 'json',
                        data: { term: request.term },
                        success: function(data) {
                            response(data);
                        },
                        error: function() {
                            response([]);
                        }
                    });
                },
                minLength: 1
            });
        });
	</script>
</body>

</html>