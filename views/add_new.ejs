        <div class="container-xxl py-5">
            <div class="container">
                
                <div class="text-center mx-auto mb-5 wow fadeInUp" data-wow-delay="0.1s" style="max-width: 600px;">
                    <h1 class="mb-3">تسجيل جديد</h1>
                </div>
                <div class="row g-4"  style="    justify-content: center;">
                   
                  
                    <div class="col-md-6">
                        <div class="wow fadeInUp" data-wow-delay="0.5s">
                            <form dir="rtl" action="/adding" method="post">
                                <div class="row g-3">
                                    <div class="col-12">
                                        <div class="form-floating">
                                            <input name="travel_details" type="text" class="form-control" id="travel_details" placeholder="تفاصيل الرحلة " required>
                                            <label for="travel_details">تفاصيل الرحلة</label>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-floating">
                                            <input name="name" type="text" class="form-control" id="name" placeholder="الأسم" required>
                                            <label for="name">الاسم</label>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-floating">
                                            <input name="passportId" type="text" class="form-control" placeholder="رقم الجواز" required>
                                            <label for="passportId">رقم الجواز</label>
                                        </div>
                                    </div>
                                    <div class="col-12">
                                        <div class="form-floating">
                                            <input name="directorName" type="text" class="form-control" id="subject" placeholder="الوكيل"required>
                                            <label for="directorName">الوكيل</label>
                                        </div>
                                    </div>
                                    <div class="col-12">
                                        <div class="form-floating">
                                            <input name="direction" type="text" class="form-control" id="subject" placeholder="الوجهة" required>
                                            <label for="direction">الوجهة</label>
                                        </div>
                                    </div>
                                    <div class="col-12">
                                        <div class="form-floating">
                                            <input name="status" type="text" class="form-control" id="subject" placeholder="الحاله" required>
                                            <label for="status">الحاله</label>
                                        </div>
                                    </div>
                                    <div class="col-12">
                                        <div class="form-floating">
                                            <input name="phoneNumber" type="tel" class="form-control" id="subject" placeholder="رقم الهاتف" >
                                            <label for="phoneNumber">رقم الهاتف</label>
                                        </div>
                                    </div>
                                    <div class="col-12">
                                        <div class="form-floating">
                                            <input name="notes"  type="text" class="form-control" id="subject" placeholder="ملاحظات ">
                                            <label for="notes"> ملاحظات</label>
                                        </div>
                                    </div>
                                    
                                    <div class="col-12">
                                        <button class="btn btn-primary w-100 py-3" type="submit">تسجيل</button>
                                    </div>
                                </div>
                                <h3 class="mb-3"> <%= complete %> </h3>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
       
        <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i class="bi bi-arrow-up"></i></a>
    </div>

    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="/lib/wow/wow.min.js"></script>
    <script src="/lib/easing/easing.min.js"></script>
    <script src="/lib/waypoints/waypoints.min.js"></script>
    <script src="/lib/owlcarousel/owl.carousel.min.js"></script>
    	<link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/themes/smoothness/jquery-ui.css">


        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
      
        <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js"></script>

    <script src="js/main.js"></script>
<script>
    
    
 
    $(function () {
			$('#travel_details').autocomplete({
				source: function (req, res) {
					$.ajax({
						url: '/autocomplet',
						dataType: "jsonp",
						typy: "GET",
						data: req,
						success: function (data) {
							res(data)
							

						},
						error: function (err) {
							console.log(err);
						}
					});
				},

				minLength: 1,
				select: function (event, ui) {
					if (ui.item) {
						$('#travel_details').text(ui.item.label)
					}
				}
			});
		},
		)
</script>



</body>

</html>